{
  "seed": 42,
  "agent": "TD3",
  "observer": "BaseObserver",
  "executer": "BaseExecuter",
  "environment": "InvertedDoublePendulumMuJoCoEnv-v0",
  "buffer_size": 1000000,
  "train_steps": 1000000,
  "initial_steps": 10000,
  "batch_size": 128,
  "eval_steps": 10,
  "actor_layer": [
      {"type": "linear", "n_neurons": [0, 128]},
      {"type": "relu"},
      {"type": "linear", "n_neurons": [128, 128]},
      {"type": "relu"},
      {"type": "linear", "n_neurons": [128, 0]},
      {"type": "tanh"}
    ],
  "critic_layer": [
    {"type": "linear", "n_neurons": [0, 128]},
    {"type": "relu"},
    {"type": "linear", "n_neurons": [128, 128]},
    {"type": "relu"},
    {"type": "linear", "n_neurons": [128, 1]}
  ],
  "actor_lr": 3e-4,
  "critic_lr": 3e-4
}